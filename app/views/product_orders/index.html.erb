<table class="table table-striped">
  <thead>
  <tr>
    <th>ID</th>
    <th>Order ID</th>
    <th>Product ID</th>
    <th>Quantity</th>
  </tr>
  </thead>
  <% @product_order.each do |productorder| %>
      <tr>
        <td><%= productorder.id %></td>
        <td><%= productorder.order_id %></td>
        <td><%= productorder.product_id %></td>
        <td><%= productorder.quantity %></td>
        <td>
          <%= link_to "Delete", productorder, method: :delete, class: 'btn btn-danger' %>
          <%= link_to "Edit", edit_product_order_path(productorder), class: 'btn btn-warning' %>

        </td>
      </tr>
  <% end %>
</table>

<%= link_to "New", new_product_order_path, class: 'btn btn-success' %>
